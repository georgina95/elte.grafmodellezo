<mvc:View
    controllerName="elte_grafmodellezo.app.controller.Second"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m">
        <Page id="page" title="{i18n>appTitle}">
            <Button text="PressMe" press="onPress" />
            <Button text="Read" press="onPressRead" />
            <List items="{catalogModel>/Authors}">
                <items>                            
                    <StandardListItem title="{catalogModel>name}"/>
                </items>
            </List>
        </Page>
</mvc:View>
