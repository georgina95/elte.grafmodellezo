<mvc:View
    controllerName="eltegrafmodellezo.app.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f."
    displayBlock="true"
    xmlns="sap.m">
    <Shell id="shell" appWidthLimited="false" title="{i18n>appTitle}" > 
		<Page title="{i18n>appTitle}" showHeader="true" showNavButton="{Settings>/showNavButton}" navButtonPress="onNavBack"
            showFooter="false" showSubHeader="false" backgroundDesign="Transparent">
            <headerContent>
                <Image 
                    height="2em"
                    src="../assets/logo.svg"
                    decorative="false"
                    press="onPressElte"/>
            </headerContent>
            <content>
                <f:FlexibleColumnLayout id="layoutMain" layout="{Settings>/layout}" backgroundDesign="Transparent"/>
            </content>
		</Page>
	</Shell>
</mvc:View>
